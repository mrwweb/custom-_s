				},
				{
					"name": "8",
					"slug": "80",
					"size": "clamp(2rem, 6.4vmin, 3.125rem)"
				},
				{
					"name": "9",
					"slug": "90",
					"size": "clamp(2.75rem, 8.65vmin, 4.125rem)"
				},
				{
					"name": "10",
					"slug": "100",
					"size": "clamp(3.6875rem, 11.68vmin, 5.625rem)"
				}
			]
		},
		"color": {
			"palette": [
				{
					"name": "",
					"slug": "background",
					"color": "#fff"
				},
				{
					"name": "",
					"slug": "foreground",
					"color": "#000"
				},
				{
					"name": "",
					"slug": "primary",
					"color": ""
				},
				{
					"name": "",
					"slug": "secondary",
					"color": ""
				},
				{
					"name": "",
					"slug": "accent",
					"color": ""
				},
				{
					"name": "Dark Gray",
					"slug": "dark-gray",
					"color": "#444"
				},
				{
					"name": "Gray",
					"slug": "gray",
					"color": "#777"
				},
				{
					"name": "Light Gray",
					"slug": "light-gray",
					"color": "#EFEFEF"
				}
			],
			"gradients": [],
			"customDuotone": false
		},
		"typography": {
			"fontFamilies": [
				{
					"name": "System Fonts",
					"slug": "system",
					"fontFamily": "-apple-system,BlinkMacSystemFont,'Segoe UI',Roboto,Oxygen-Sans,Ubuntu,Cantarell,'Helvetica Neue',sans-serif"
				},
				{
					"name": "Monospace",
					"slug": "monospace",
					"fontFamily": "ui-monospace, 'Cascadia Code', 'Source Code Pro', Menlo, Consolas, 'DejaVu Sans Mono', monospace;"
				}
			],
			"fontSizes": [
				{
					"name": "Tiny",
					"slug": "x-small",
					"size": "0.875rem",
					"fluid": {
						"min": ".875rem",
						"max": "1.75rem"
					}
				},
				{
					"name": "Small",
					"slug": "small",
					"size": "1.0625rem",
					"fluid": {
						"min": "0.9375rem",
						"max": "2.125rem"
					}
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "1.25rem",
					"fluid": {
						"min": "1.125rem",
						"max": "2.5rem"
					}
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "1.5rem",
					"fluid": {
						"min": "1.375rem",
						"max": "3rem"
					}
				},
				{
					"name": "Extra-Large",
					"slug": "x-large",
					"size": "2rem",
					"fluid": {
						"min": "1.75rem",
						"max": "4rem"
					}
				},
				{
					"name": "XXL",
					"slug": "xx-large",
					"size": "2.5rem",
					"fluid": {
						"min": "2.25rem",
						"max": "5rem"
					}
				},
				{
					"name": "XXXL",
					"slug": "xxx-large",
					"size": "3.375rem",
					"fluid": {
						"min": "2.5rem",
						"max": "6.75rem"
					}
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.4"
		},
		"color": {
			"text": "var(--wp--preset--color--base)",
			"background": "var(--wp--preset--color--contrast)",
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": ""
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":active": {
					"typography": {
						"textDecoration": "none"
					}
				}
			},
			"button": {
				"border": {
					"radius": ""
				},
				"color": {
					"background":,
					"text":
				},
				":hover" {
					"color": {
						"background":,
						"text":
					}
				},
				":focus" {
					"color": {
						"background":,
						"text":
					}
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--70)"
		}
	},
	"templateParts": [
        {
            "area": "footer",
            "name": "footer",
            "title": "Footer"
        }
    ]
}
